index ix,iy,iz;


mass p1=mm, p2=mm, p3=mm, p4=mm, k1=0;

 
mshell p1,p2,p3,p4,k1;


vector q1,q2;

 
operator ga,gb;


for all p let ga(p)=g(la,p) + mm,
              gb(p)=g(lb,p) + mm;


xx := g(la,ix)*g(la,iy)*(g(lb,ix)*gb(p1)*g(lb,iy)*gb(q2) +
                      gb(p3)*g(lb,ix)*g(lb,iy));


