@TECHREPORT{Broadhurst:96,
  author = {D. J. Broadhurst},
  title = {On the enumeration of irreducible $\kappa$-fold Euler sums and their
	roles in knot theory and field theory},
  institution = {Open University, Milton Keynes MK7 6AA, England},
  year = {1996},
  number = {OUT-4102-62},
  month = {April},
  abstract = {A generating function is given for the number, $\cal E(l,k)$, of irreducible
	$\kappa$-fold Euler sums, with all possible alternations of sign,
	and exponents summing to $l$. Its form is remarkably simple: $\Sum_\cal
	E(k + 2n, k)x^n$ = $\Sum_d|k$ $\mu(d)$ $(1 - x^d)^k/d$ $/k$, where
	$\mu$ is the M\"{o}bius function. Equivalently, the size of the search
	space in which $\kappa$-fold Euler sums of level l are reducible
	to rational linear combinations of irreducible basis terms is $\cal
	S(l, k)$ = $\Sum_n<k Analytical methods, using Tony Hearn's REDUCE,
	achieve this reduction for the 3698 convergent double Euler sums
	with $l\leq 44$; numerical methods using David Bailey's MPPSLQ, achieve
	it for the 1457 convergent $\cal$-fold sums with $l\leq 7$; combined
	methods yield bases for all remaining search spaces with $\cal S(l,
	k)$ \leq 34$. These findings confirm expectations based on Dirk Kreimer's
	connection of knot theory with quantum field theory. The occurrence
	in perturbative quantum electrodynamics of all 12 irreducible Euler
	sums with $l \leq 7$ is demonstrated. It is suggested that no further
	transcendental occurs in the four-loop contributions to the electron's
	magnetic moment. Irreducible Euler sums are found to occur in explicit
	analytical results, for counterterms with up to 13 loops, yielding
	transcendental knot-numbers, up to 23 crossings.}
}

