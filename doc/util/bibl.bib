@ARTICLE{Viry:93,
  author = {Guy Viry},
  title = {Factorization of Multivariate Polynomials with Coefficients in ${F}_{p}$},
  journal = {Journal of Symbolic Computation},
  year = {1993},
  volume = {15},
  pages = {371-391},
  number = {4},
  month = {November},
  comment = {The ring of polynomials in $X,X_{1}, \dots ,X_{m}$ are denoted by
	$F_{p}[X,X_{1}, \dots ,X_{m}]$ in $F_{p}$, that is the field of integers
	defined modulo $p$. In the usual factorization algorithm defined
	by Wang, the given polynolmial $P$ is first factorized modulo $\Delta^{n}$,
	where $\Delta^{n}$ is an ideal. This algorithm uses a generalization
	of Hensel's Lemma. If there are no extraneous factors, then the factors
	defined modulo $\delta^[{n}$ correspond to the factors of $P$ in
	$F_{p}[X,X_{1}, \dots ,X_{m}]$ or else the defined factors must be
	regrouped to find the factors of $P$ in $F_{p}[X,X_{1}, \dots ,X_{m}]$.
	In this paper, a mapping that transforms the product of the factors
	into a sum is defined. A theorem that determines whether a subproduct
	of the factors of $P$ corresponds to a factor of $P$ in $F_{p}[X,X_{1},
	\dots ,X_{m}]$ is given. Therefore the regrouping of the factors
	of $P$ reduces to solving a system of linear equations, as in the
	univariate case, with Berlekamp's algorithm.},
  url = {http://dx.doi.org/10.1006/jsco.1993.1027}
}

