  volume = {72},
  series = {Lecture Notes in Computer Science},
  pages = {398-407},
  publisher = {Springer-Verlag},
  abstract = {By the end of the 1960s it had been shown that a computer could find
	indefinite integrals with a competence exceeding that of typical
	undergraduates. This practical advance was backed up by algorithmic
	interpretations of a number of classical results on integration,
	and by some significant mathematical extensions to these same results.
