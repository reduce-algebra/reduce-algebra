  Crossref                 = {ISSAC:2008},
  Doi                      = {10.1145/1390768.1390784},
  Keywords                 = {boolean ring, groebner basis, involutive algorithm, pommaret division},
  Location                 = {Linz/Hagenberg, Austria},
  Numpages                 = {8},
  Url                      = {http://doi.acm.org/10.1145/1390768.1390784}
}

