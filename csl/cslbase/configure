main ()
{
printf("\n");
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: Will use --hash-style=both" >&5
$as_echo "$as_me: Will use --hash-style=both" >&6;}
else
  LDFLAGS="$OLDLDFLAGS"
     { $as_echo "$as_me:${as_lineno-$LINENO}: --hash-style=both not available" >&5
$as_echo "$as_me: --hash-style=both not available" >&6;}
