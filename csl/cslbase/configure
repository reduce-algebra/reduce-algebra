  then
     OLDCFLAGS="$CFLAGS"
     CFLAGS="$CFLAGS -m32"
     cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */
#include <stdio.h>
int
main ()
{
printf("\n");
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: Using -m32 to force 32-bit compilation" >&5
$as_echo "$as_me: Using -m32 to force 32-bit compilation" >&6;}
        CXXFLAGS="$CXXFLAGS -m32"
        LDFLAGS="$LDFLAGS -m32"
else
  CFLAGS="$OLDCPPFLAGS"
         { $as_echo "$as_me:${as_lineno-$LINENO}: This version of gcc does not accept -m32" >&5
$as_echo "$as_me: This version of gcc does not accept -m32" >&6;}
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
  else
    { $as_echo "$as_me:${as_lineno-$LINENO}: --with-m32 specified but not using gcc" >&5
$as_echo "$as_me: --with-m32 specified but not using gcc" >&6;}
  fi
fi

